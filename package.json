{
  "name": "podcast-app",
  "version": "0.0.1",
  "scripts": {
    "installAll": "npm install & npm install --prefix podcast-feed-loader/app & npm install --prefix website-react",
    "CiAll": "npm ci & npm ci --prefix podcast-feed-loader/app & npm ci --prefix website-react",
    "lint": "standard --fix",
    "precommit": "standard",
    "testFeedLoader": "npm run test --prefix podcast-feed-loader/app",
    "testWebsite": "npm run testNonInteractive --prefix website-react",
    "e2e": "npm run ci --prefix website-react",
    "concurrentlyScripts": "concurrently npm:e2e standard npm:testFeedLoader npm:testWebsite --kill-others-on-fail"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "jest-dom": "^3.0.0",
    "service-worker-mock": "^1.11.1"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "react-testing-library": "^5.4.2",
    "standard": "^11.0.1"
  },
  "contributors": [
    "https://github.com/THEjoezack",
    "https://github.com/nmarcora",
    "https://github.com/Madsn",
    "https://github.com/AdamLantz",
    "https://github.com/benjaminsteward",
    "https://github.com/MadVikingGod",
    "https://github.com/vlado92",
    "https://github.com/davefollett",
    "https://github.com/ArleneAndrews"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/",
      "**/build/",
      "**/coverage/"
    ],
    "globals": [
      "cy",
      "Cypress"
    ],
    "env": {
      "jest": true
    }
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "description": "![qit logo](https://github.com/codingblocks/podcast-app/blob/master/website-react/public/images/icons/icon-144x144.png)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArleneAndrews/podcast-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArleneAndrews/podcast-app/issues"
  },
  "homepage": "https://github.com/ArleneAndrews/podcast-app#readme"
}
